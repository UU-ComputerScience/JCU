@import "reset"

body
  background #A1AFC9
  font bold 18px "Helvetica Neue", Arial, Helvetica, sans-serif

ul
  list-style-type none

a, a:visited
  color #DB672E
  text-decoration none

#home-view
  width 1000px
  height 100%
  margin 20px auto

  #content
    background #FFF
    padding 20px
    -webkit-border-radius 4px
    -moz-border-radius 4px
    border-radius 4px
